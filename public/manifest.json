{
  "manifest_version": 3,
  "short_name": "Time Keeper",
  "name": "Time Keeper",
  "version": "0.0.1",
  "icons": {
    "128": "logo128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Time Keeper"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
